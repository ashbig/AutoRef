ULTFILEREFERENCE  (RESULTID NUMBER(10,0) NOT NULL  ,FILEREFERENCEID NUMBER(10,0) NOT NULL );  CREATE TABLE NAME  (SEQUENCEID NUMBER(10,0) NOT NULL  ,NAMETYPE VARCHAR2(30) NOT NULL  ,NAMEVALUE VARCHAR2(240) NOT NULL  ,NAMEURL VARCHAR2(240)  ,DESCRIPTION VARCHAR2(4000) );  CREATE TABLE REFSEQUENCE  (SEQUENCEID NUMBER(10,0) NOT NULL  ,GENUSSPECIES NUMBER(1) NOT NULL  ,CDSSTART NUMBER(38)  ,CDSSTOP NUMBER(38)  ,GCCONTENT NUMBER  ,CDNASOURCE VARCHAR2(1000)  ,CHROMOSOME VARCHAR2(20)  ,DATEADDED DATE NOT NULL );   CREATE TABLE RESULT  (RESULTID NUMBER(10,0) NOT NULL  ,SAMPLEID NUMBER(10,0) NOT NULL  ,RESULTTYPE NUMBER(2) NOT NULL  ,RESULTVALUEID NUMBER(10,0) );  CREATE TABLE FILEREFERENCE  (FILEREFERENCEID NUMBER(10,0) NOT NULL  ,LOCALPATH VARCHAR2(250) NOT NULL  ,BASENAME VARCHAR2(250)  ,FILETYPE NUMBER(1) NOT NULL );  CREATE TABLE USERPROFILE  (USERID NUMBER(10,0) NOT NULL  ,USERNAME VARCHAR2(30) NOT NULL  ,USEREMAIL VARCHAR2(240) NOT NULL  ,USERPASSWORD VARCHAR2(240) NOT NULL  ,USERGROUP VARCHAR2(30) NOT NULL  ,USERORGANIZATION VARCHAR2(240)  ,USERINFORMATION VARCHAR2(240)  ,PASSWORDREMINDERSTRING VARCHAR2(50) NOT NULL );  CREATE TABLE OLIGOCONTAINER  (OLIGOCONTAINERID NUMBER(10,0) NOT NULL  ,LABEL VARCHAR2(12) NOT NULL  ,USERID NUMBER(10,0)  ,ORDERDATE DATE NOT NULL  ,STATUS INTEGER DEFAULT 0 NOT NULL  ,DESCR_OLIGO_PROCESSING VARCHAR2(2000)  ,DESCR_SEQUENCING VARCHAR2(2000) );  CREATE TABLE PROCESS_OBJECT  (OBJECTID NUMBER(10,0) NOT NULL  ,OBJECTTYPE NUMBER(1) NOT NULL  ,PROCESSID NUMBER(10,0) NOT NULL ); CREATE TABLE STRETCH  (SEQUENCESTART INTEGER DEFAULT -1000  ,SEQUENCESTOP INTEGER DEFAULT -1000  ,STRETCHID NUMBER(10,0) NOT NULL  ,STATUS INTEGER DEFAULT 1 NOT NULL  ,SEQUENCEID NUMBER(10,0)  ,CDSSTART INTEGER NOT NULL  ,CDSEND INTEGER NOT NULL  ,ORIENTATION NUMBER(10,0) DEFAULT 0 NOT NULL  ,TYPE INTEGER NOT NULL  ,ANALYSISSTATUS INTEGER DEFAULT 0  ,COLLECTIONID NUMBER(10,0) NOT NULL );  C