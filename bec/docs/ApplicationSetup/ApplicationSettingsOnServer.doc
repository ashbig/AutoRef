RATEGYIDFROM ISOLATETRACKING ISG    ,FLEXINFO FLO    ,SEQUENCINGCONSTRUCT AGG  WHERE flo.isolatetrackingid=isg.isolatetrackingid and agg.constructid=isg.constructid;  CREATE OR REPLACE FORCE VIEW VIEW_FLEXBECSEQUENCEID (FLEXSEQUENCEID ,BECSEQUENCEID) AS SELECT FLO.FLEXSEQUENCEID FLEXSEQUENCEID          ,AGG.REFSEQUENCEID BECSEQUENCEIDFROM FLEXINFO FLO    ,ISOLATETRACKING ISG    ,SEQUENCINGCONSTRUCT AGG  WHERE flo.isolatetrackingid=isg.isolatetrackingid and isg.constructid=agg.constructid//****************************** for external use onlyCREATE TABLE TEMP_SEQUENCE_ID (USERSEQUENCEID NUMBER(10,0) NOT NULL ,APPSSEQUENCEID NUMBER(10,0) NOT NULL );ALTER TABLE TEMP_SEQUENCE_ID  ADD (CONSTRAINT TSD_PK PRIMARY KEY   (USERSEQUENCEID));/// insert into nametype values ('GENBANK_ACCESSION'); insert into nametype values ('GI'); insert into nametype values ('UNIGENE_SID'); insert into nametype values ('GENE_SYMBOL'); insert into nametype values ('SGD'); insert into nametype values ('ALT_GENE_NAME'); insert into nametype values ('ALT_PROTEIN_NAME'); insert into nametype values ('FUNCTION'); insert into nametype values ('GENE_NAME'); insert into nametype values ('GENOMIC_CONTEXT'); insert into nametype values ('GI_PROTEIN'); insert into nametype values ('HOMOLOGY'); insert into nametype values ('PANUMBER'); insert into nametype values ('PATHWAYS'); insert into nametype values ('PRIMARY_FUNCTION'); insert into nametype values ('RATING'); insert into nametype values ('STRUCTURE_FEATURE'); insert into nametype values ('LOCUS_ID'); insert into nametype values ('MGC_ID'); insert into nametype values ('IMAGE_ID'); insert into nametype values ('RZPD_CLONEID'); insert into nametype values ('COG'); insert into nametype values ('ORF_STATUS'); insert into nametype values ('ORF_TYPE'); insert into nametype values ('PID'); insert into nametype values ('PRODUCT'); insert into nametype values ('SEQUENCE_TYPE'); insert into nametype values ('STRA