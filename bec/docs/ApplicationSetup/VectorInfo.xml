<vector-info>


   
    <!-- Vector data -->
    <!-- vector type 0  master , 1  expression -->
    
  <vector>
    <vector-name>name</vector-name>
    <vector-source>source</vector-source>
    <vector-type>1</vector-type>
    <vector-filename>filename</vector-filename>
    <vector-filepath>filepath</vector-filepath>
    <vector-feature >
        <feature-name>feature name</feature-name>
        <feature-description>feature description</feature-description>
        <!-- feature type -> 1 - added; 0 - remain; -1 - lost -->
        <feature-type>1</feature-type>
    </vector-feature>
    <vector-feature >
            <feature-name>feature name 1</feature-name>
            <feature-description>feature description 1</feature-description>
             <feature-type>-1</feature-type>
    </vector-feature>
    <vector-feature >
            <feature-name>feature name 2</feature-name>
            <feature-description>feature description 21</feature-description>
            <!-- feature type -> 1 - added; 0 - remain; -1 - lost -->
            <feature-type>0</feature-type>
    </vector-feature>
   
  </vector>
  <vector>
      <vector-name>name 1</vector-name>
      <vector-source>source 1</vector-source>
