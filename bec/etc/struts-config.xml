<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.0//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_0.dtd">

<!--
     This is the Struts configuration file for the example application,
     using the proposed new syntax.

     NOTE:  You would only flesh out the details in the "form-bean"
     declarations if you had a generator tool that used them to create
     the corresponding Java classes for you.  Otherwise, you would
     need only the "form-bean" element itself, with the corresponding
     "name" and "type" attributes.
-->


<struts-config>


 
  <!-- ========== Form Bean Definitions =================================== -->
  <form-beans>

    <!-- Logon form bean -->
    <form-bean      name="logonForm"
                    type="edu.harvard.med.hip.bec.form.LogonForm"/>
      <!-- Customer registration form bean -->
    <form-bean      name="customerRegistrationForm"
                    type="edu.harvard.med.hip.bec.form.CustomerRegistrationForm"/>
     <!-- Find registration form bean -->
    <form-bean      name="findRegistrationForm"
                    type="edu.harvard.med.hip.bec.form.FindRegistrationForm"/>           
 <!-- Form used to do the sequencing project -->
    <form-bean      name="seqGetSpecForm"
                    type="edu.harvard.med.hip.bec.form.Seq_GetSpecForm"/>
    <!-- Form used to do the sequencing project -->
    <form-bean      name="seqSequenceAnalysisRequestForm"
                    type="edu.harvard.med.hip.bec.form.Seq_SequenceAnalysisRequestForm"/>
  <!-- Customer receive  orders form bean -->
    <form-bean      name="ReceiveOrdersForm"
                    type="edu.harvard.med.hip.bec.form.ReceiveOrdersForm"/>

 <!-- Customer receive  orders form bean -->
    <form-bean      name="submitDataFileForm"
                    type="edu.harvard.med.hip.bec.form.SubmitDataFileForm"/>

 <!-- Form used to run a process  -->
   <!-- <form-bean      name="seqRunProcessForm" type="edu.harvard.med.hip.bec.form.RunProcessForm"/> -->



 </form-beans>



  <!-- ========== Global Forward Definitions ============================== -->
  <global-forwards>
    <forward    name="help"         path="/Help.jsp"/>
    <forward    name="login"        path="/login.jsp"/>
    <forward    name="logout"       path="/Logout.do"/>
    <forward    name="home"         path="/overview.jsp"/>
    <forward    name="error"        path="/ProcessError.do"/>
   <forward    name="register"     path="/CustomerRegistration.jsp"/>
    <forward    name="findRegistration" path="/FindRegistration.jsp"/>
 

    <forward    name="seqEnterParams" path="/Seq_Parameters.jsp"/>
    <forward    name="seqSelectProcess" path="/SelectProcess.jsp"/>
    <forward    name="seqSequenceAnalysis" path="/Seq_FullSequenceAnalysisRequest.jsp"/>

  </global-forwards>
  

  <!-- ========== Action Mapping Definitions ============================== -->
  <action-mappings type="org.apache.struts.action.ActionMapping">

    
    <!-- Process a user logon -->
    <action    path="/logon"
               type="edu.harvard.med.hip.bec.action.LogonAction"
               name="logonForm"
              scope="request"
              input="/login.jsp">
      <forward  name="success"   path="/about_bec.jsp"/>
      <forward  name="failure"  path="/login.jsp"/>
    </action>

    <!-- process a user logout -->
    <action     path="/Logout"
                type="edu.harvard.med.hip.bec.action.LogoutAction">
      <forward  name="success"  path="/login.jsp"/>
   </action>

   

    <action     path="/ProcessError"
                type="edu.harvard.med.hip.bec.action.ProcessErrorAction">
       <forward name="displayError" path="/BecError.jsp"/>
                
    </action>


     <!-- Process a find registration -->
    <action    path="/FindRegistration"
               type="edu.harvard.med.hip.bec.action.FindRegistrationAction"
               name="findRegistrationForm"
              input="/FindRegistration.jsp">
      <forward  name="success"   path="/FindRegistrationConfirm.jsp"/>
      <forward  name="failure"  path="/FindRegistration.jsp"/>
    </action>

 <!-- Process a customer registration -->
    <action    path="/CustomerRegistration"
               type="edu.harvard.med.hip.bec.action.CustomerRegistrationAction"
               name="customerRegistrationForm"
              input="/CustomerRegistration.jsp">
      <forward  name="success"   path="/RegistrationConfirm.jsp"/>
      <forward  name="failure"  path="/CustomerRegistration.jsp"/>
    </action>
  
    
 <!-- Process display specs -->
    <action      path="/Seq_GetSpec"
                name="seqGetSpecForm"
                type="edu.harvard.med.hip.bec.action.Seq_GetSpecAction">
            <forward name="end_reads_spec" path="/Seq_DisplayEndReadsParameters.jsp"/>
            <forward name="full_seq_spec" path="/Seq_DisplayFullSeqParameters.jsp"/>
             <forward name="primer3_spec" path="/Seq_DisplayPrimerParameters.jsp"/>
             <forward name="polymorphism_spec" path="/Seq_DisplayPolymorphismParameters.jsp"/>
            <forward name="slidingwindow_spec" path="/Seq_DisplaySlidingWindowParameters.jsp"/>
            <forward name="universal_pair" path="/Seq_DisplayUniversalPrimerParameters.jsp"/>
    </action>

 <!-- Process display item -->
    <action     path="/Seq_GetItem"
                name="seqGetSpecForm"
                scope="request"
                input="/ContainerScan.jsp"
                type="edu.harvard.med.hip.bec.action.Seq_GetItemAction">
            <forward name="display_vector" path="/Display_Vector.jsp"/>
            <forward name="display_linker" path="/Display_Linker.jsp"/>
            <forward name="display_refsequence_details" path="/Display_RefSequence.jsp"/>
            <forward name="display_container_results_er" path="/ContainerResultsEndReads.jsp"/>
            <forward name="display_clone" path="/CloneInfoDisplay.jsp"/>
            <forward name="display_cloning_strategy" path="/Display_CloningStrategy.jsp"/>
            <forward name="display_container_details" path="/ContainerDetails.jsp"/>
            <forward name="display_container_history" path="/ContainerHistory.jsp"/>
            <forward name="display_discrepancyreport" path="/Display_Discrepancies.jsp"/>
            <forward name="display_scoredsequence_details" path="/Display_ScoredSequence.jsp"/>
            <forward name="display_needle_alignment" path="/NeedleAlignment.jsp"/>
            <forward name="display_isolate_ranker_report" path="/ContainerQualityReport.jsp"/>
             <forward name="display_sample_isolate_ranker_report" path="/Display_Sample.jsp"/>
            <forward name="display_read_report" path="/Display_Read.jsp"/>
             <forward name="show_activate_list" path="/CloneList.jsp"/>
            <forward name="show_clone_status_list" path="/ContainerCloneStatus.jsp"/>
             <forward name="construct_report" path="/ConstructReport.jsp"/>
              <forward name="display_clone_sequence" path="/Display_CloneSequence.jsp"/>
             <forward name="display_info" path="/ConfirmProcess.jsp"/>
             <forward name="display_oligo_container" path  = "/Display_OligoContainer.jsp"/>
            <forward name="display_stretch_report" path  = "/Display_Stretch.jsp"/>
            <forward name="display_stretch_collections" path  = "/Display_StretchCollections.jsp"/>   
       
</action>

 

<!-- Process select process to run -->
    <action      path="/SelectProcess"
                name="seqGetSpecForm"
                type="edu.harvard.med.hip.bec.action.SelectProcessAction">
            <forward name="upload_plates" path="/RunUploadPlates.jsp"/>
             <forward name="run_desicion_tool" path="/RunDesicionTool.jsp"/>
            <forward name="select_vector" path="/SelectVector.jsp"/>
            <forward name="initiate_process" path="/InitiateProcess.jsp"/>
            <forward name="scan_label" path="/ContainerScan.jsp"/>
            <forward name="submit_data_file" path="/SubmitDataFile.jsp"/>
            <forward name="select_plates" path="/SelectPlatesForProcess.jsp"/>
            <forward name="processing" path="/ConfirmProcess.jsp"/>
            <forward name="run_report" path="/RunReport.jsp"/>
    </action>

<!-- Process run discrepancy finder as stand aloan -->
    <action      path="/SubmitDataFile"
                name="submitDataFileForm"
                input="/SubmitDataFile.jsp"
                type="edu.harvard.med.hip.bec.action.SubmitDataFileAction">
            <forward name="response" path="/Response.jsp"/>
            <forward name="processing" path="/ConfirmProcess.jsp"/>
    </action>



<!-- Process  run a process-->
    <action      path="/RunProcess"
                name="seqGetSpecForm"
                type="edu.harvard.med.hip.bec.action.RunProcessAction">
            <forward name="processing" path="/ConfirmProcess.jsp"/>
            <forward name="decision_tool_report" path="/DecisionToolReport.jsp"/>
            <forward name="display_item_history" path="/Display_ItemHistory.jsp"/>
            <forward name="display_oligo_calculations" path="/Display_OligoCalculation.jsp"/>
<forward name="display_oligo_container" path  = "/Display_OligoContainer.jsp"/>
       <forward name="display_stretch_collections" path  = "/Display_StretchCollections.jsp"/>   
           <forward name="display_lqr_collections" path  = "/Display_LQRCollections.jsp"/>  
           <forward name="display_oligo_order_for_clone" path  = "/Display_OligoOrderForClone.jsp"/> 
             
    </action>

  <!-- Process the receiving of new  plates -->
    <action    path="/ReceivePlates"
               type="edu.harvard.med.hip.bec.action.ReceivePlatesAction"
               name="ReceiveOrdersForm"
               scope="request"
               input="/ReceiveOrders.jsp">
     <!-- <forward  name="success"   path="/EnterPlateLocation.jsp"/>-->
    </action>

<!-- Process create specs specs -->
    <action      path="/Seq_SubmitSpec"
                name="seqGetSpecForm"
                type="edu.harvard.med.hip.bec.action.Seq_SubmitSpecAction">
            <forward name="end_reads_spec" path="/Seq_DisplayEndReadsParameters.jsp"/>
            <forward name="full_seq_spec" path="/Seq_DisplayFullSeqParameters.jsp"/>
             <forward name="primer3_spec" path="/Seq_DisplayPrimerParameters.jsp"/>
             <forward name="polymorphism_spec" path="/Seq_DisplayPolymorphismParameters.jsp"/>
            <forward name="universal_pair" path="/Seq_DisplayUniversalPrimerParameters.jsp"/>
           <forward name="slidingwindow_spec" path="/Seq_DisplaySlidingWindowParameters.jsp"/>
         
    </action>
<!-- Process process trace files -->
    <action      path="/TraceFileProcessing"
                name="submitDataFileForm"
input ="/TraceFileProcessing.jsp"
                type="edu.harvard.med.hip.bec.action.TraceFileProcessingAction">
           <forward name="processing" path="/ConfirmProcess.jsp"/>
        
</action>



    <!-- The standard administrative actions available with Struts -->
    <!-- These would be either omitted or protected by security -->
    <!-- in a real application deployment -->
    <action    path="/admin/addFormBean"
               type="org.apache.struts.actions.AddFormBeanAction"/>
    <action    path="/admin/addForward"
               type="org.apache.struts.actions.AddForwardAction"/>
    <action    path="/admin/addMapping"
               type="org.apache.struts.actions.AddMappingAction"/>
    <action    path="/admin/reload"
               type="org.apache.struts.actions.ReloadAction"/>
    <action    path="/admin/removeFormBean"
               type="org.apache.struts.actions.RemoveFormBeanAction"/>
    <action    path="/admin/removeForward"
               type="org.apache.struts.actions.RemoveForwardAction"/>
    <action    path="/admin/removeMapping"
               type="org.apache.struts.actions.RemoveMappingAction"/>


  </action-mappings>

</struts-config>
