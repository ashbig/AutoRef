<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
     PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<file_maps>
<file_map file_type="FILE_TYPE_ONE_FILE_SUBMISSION" max_number_of_column_per_property="2">
    
    <file_column header="CLONE_ID" object="SAMPLE"   property_name="USER_ID" />

    <file_column header="CLONE_COLLECTION" object="CONTAINER" property_name="LABEL" property_order="0" />
    <file_column header="PLATE_NUM" object="CONTAINER" property_name="LABEL" property_order ="1" />
    
    <file_column header="ROW" object="SAMPLE" property_name="POSITION" property_order ="0"/>
    <file_column header="COL" object="SAMPLE" property_name="POSITION" property_order ="1"/>
  
    <file_column key="1" header="PARENT_GB_ACCNUM" object="FLEXSEQUENCE"
                    property_name ="SEARCHID"
                    property_instruction ="http://eutils.ncbi.nlm.nih.gov/entrez/eutils/efetch.fcgi?db=nuccore&amp;retmode=xml&amp;id=" />
    
    
    <file_column header="STOP_CODON_STATUS" object="SAMPLE" property_name ="CONSTRUCT_TYPE">
        <column_translation file_value="without" translated_value="FUSION" />
        <column_translation file_value="with" translated_value="CLOSED" />
    </file_column>

    <file_column header="VECTOR_NAME" object="CLONE" object_property_type="NAMEPROPERTY" property_name="VECTOR" isSubmit="0"/>
    <file_column header="5'_LINKER" object="CLONE" object_property_type="NAMEPROPERTY" property_name="FIVE_PRIME_LINKER" isSubmit="0"/>
    <file_column header="3'_LINKER" object="CLONE" object_property_type="NAMEPROPERTY" property_name="THREE_PRIME_LINKER" isSubmit="0"/>

    <file_column header="LIBR_NAME" object="CONTAINER" object_property_type="NAMEPROPERTY" property_name="LIBR_NAME"/>



<!--  name of this property hardcoded , do not change -->

<file_column header="CloneStatus" object="CLONE" object_property_type="NAMEPROPERTY" property_name ="CLONE_STATUS" isSubmit="0"/>
<!--  name of this property hardcoded , do not change -->
 
 <file_column header="CloneType" object="CLONE" object_property_type="NAMEPROPERTY" property_name ="CLONE_TYPE" isSubmit="0"/>
<!--  name of this property hardcoded , do not change -->

<file_column header="CloneStorage" object="CLONE" object_property_type="NAMEPROPERTY" isSubmit="0" property_name ="CLONE_STORAGE_TYPE_0">
    <column_translation file_value="Original" translated_value="Original Storage" />
</file_column>

<!--  name of this property hardcoded , do not change -->
 
<file_column header="CloneStorageForm" object="CLONE" object_property_type="NAMEPROPERTY" isSubmit="0" property_name ="CLONE_STORAGE_FORM">
    <column_translation file_value="Glycerol" translated_value="GLYCEROL"  />
</file_column>


</file_map>
</file_maps>


