<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.0//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_0.dtd">

<!--
     This is the Struts configuration file for the example application,
     using the proposed new syntax.

     NOTE:  You would only flesh out the details in the "form-bean"
     declarations if you had a generator tool that used them to create
     the corresponding Java classes for you.  Otherwise, you would
     need only the "form-bean" element itself, with the corresponding
     "name" and "type" attributes.
-->


<struts-config>


 
  <!-- ========== Form Bean Definitions =================================== -->
  <form-beans>

    <form-bean      name="refseqSearchForm"
                    type="plasmid.form.RefseqSearchForm"/>
                    
    <form-bean      name="cloneDetailForm"
                    type="plasmid.form.CloneDetailForm"/>

    <form-bean      name="vectorDetailForm"
                    type="plasmid.form.VectorDetailForm"/>

    <form-bean      name="viewCartForm"
                    type="plasmid.form.ViewCartForm"/>

    <form-bean      name="logonForm"
                    type="plasmid.form.LogonForm"/>
      
    <form-bean      name="logoutForm"
                    type="plasmid.form.LogoutForm"/>
                                  
    <form-bean      name="registrationForm"
                    type="plasmid.form.RegistrationForm"/>
                                  
    <form-bean      name="checkoutForm"
                    type="plasmid.form.CheckoutForm"/>
                    
    <form-bean      name="mergeCartForm"
                    type="plasmid.form.MergeCartForm"/>
                    
    <form-bean      name="viewOrderHistoryForm"
                    type="plasmid.form.ViewOrderHistoryForm"/>
                    
    <form-bean      name="viewOrderDetailForm"
                    type="plasmid.form.ViewOrderDetailForm"/>
                    
    <form-bean      name="viewOrderClonesForm"
                    type="plasmid.form.ViewOrderClonesForm"/>
                    
    <form-bean      name="findPasswordForm"
                    type="plasmid.form.FindPasswordForm"/>
                    
    <form-bean      name="downloadClonesForm"
                    type="plasmid.form.DownloadClonesForm"/>
                    
    <form-bean      name="changeOrderStatusForm"
                    type="plasmid.form.ChangeOrderStatusForm"/>
                    
    <form-bean      name="labelPlatesForm"
                    type="plasmid.form.LabelPlatesForm"/>
                    
    <form-bean      name="printLabelsForm"
                    type="plasmid.form.PrintLabelsForm"/>
                    
    <form-bean      name="uploadCultureResultsForm"
                    type="plasmid.form.UploadCultureResultsForm"/>
                    
    <form-bean      name="generateWorklistForm"
                    type="plasmid.form.GenerateWorklistForm"/>
                    
    <form-bean      name="enterResultsForm"
                    type="plasmid.form.EnterResultsForm"/>
                    
    <form-bean      name="viewCollectionForm"
                    type="plasmid.form.ViewCollectionForm"/>
                    
    <form-bean      name="vectorSearchForm"
                    type="plasmid.form.VectorSearchForm"/>
                    
    <form-bean      name="cloneSearchForm"
                    type="plasmid.form.CloneSearchForm"/>
                    
    <form-bean      name="advancedSearchForm"
                    type="plasmid.form.AdvancedSearchForm"/>
                    
    <form-bean      name="processShippingForm"
                    type="plasmid.form.ProcessShippingForm"/>
                    
    <form-bean      name="viewContainersForm"
                    type="plasmid.form.ViewContainersForm"/>
                    
    <form-bean      name="cancelOrderForm"
                    type="plasmid.form.CancelOrderForm"/>
                    
    <form-bean      name="searchOrderForm"
                    type="plasmid.form.SearchOrderForm"/>
                    
    <form-bean      name="batchOrderForm"
                    type="plasmid.form.BatchOrderForm"/>
                    
 </form-beans>

  <!-- ========== Global Forward Definitions ============================== -->
  <global-forwards>
    <forward    name="help"         path="/Help.jsp"/>
    <forward    name="login"        path="/Login.jsp"/>
    <forward    name="logout"       path="/Logout.do"/>
    <forward    name="home"         path="/overview.jsp"/>
    <forward    name="register"     path="/CustomerRegistration.jsp"/>
    <forward    name="findRegistration" path="/FindRegistration.jsp"/>
    <forward    name="error"        path="/ProcessError.do"/>
  </global-forwards>
  

  <!-- ========== Action Mapping Definitions ============================== -->
  <action-mappings type="org.apache.struts.action.ActionMapping">
    
    <action     path="/AddCollectionToCart"
                type="plasmid.action.AddCollectionToCartAction"
               name="refseqSearchForm"
              validate="false"
              input="/ViewCollection.jsp">
      <forward  name="success"  path="/ViewCollection.jsp"/>
    </action>
    
    <action     path="/AdvancedSearch"
                type="plasmid.action.AdvancedSearchAction"
               name="advancedSearchForm"
              input="/AdvancedSearch.jsp">
      <forward  name="success"  path="/DisplayVectorSearchResults.jsp"/>
      <forward  name="empty"  path="/DisplayEmptySearchResults.jsp"/>
    </action>
    
    <action     path="/BatchOrder"
                type="plasmid.action.BatchOrderAction"
               name="batchOrderForm"
               scope="request"
              input="/BatchOrder.jsp">
      <forward  name="success"  path="/DisplayCart.jsp"/>
    </action>
    
    <action     path="/CancelOrder"
                type="plasmid.action.CancelOrderAction"
               name="cancelOrderForm"
              input="/OrderList.jsp">
      <forward  name="success"  path="/ViewOrderHistory.do"/>
    </action>
    
    <action     path="/ChangeOrderStatus"
                type="plasmid.action.ChangeOrderStatusAction"
               name="changeOrderStatusForm"
              input="/OrderList.jsp">
      <forward  name="success"  path="/ChangeOrderStatusSuccess.jsp"/>
    </action>
    
    <action     path="/Checkout"
                type="plasmid.action.CheckoutAction"
               name="checkoutForm"
              input="/UpdateCart.do">
      <forward  name="success"  path="/ChooseShipping.jsp"/>
    </action>
      
    <action     path="/CheckoutConfirm"
                type="plasmid.action.CheckoutConfirmAction"
               name="checkoutForm"
              input="/Checkout.jsp">
      <forward  name="success"  path="/CheckoutConfirm.jsp"/>
    </action>
    
    <action     path="/ConfirmLogout"
                type="plasmid.action.ConfirmLogoutAction"
               name="logoutForm"
              scope="request"
              input="/Logout.do">
      <forward  name="success"  path="/Home.jsp"/>
    </action>
    
    <action     path="/ConfirmResults"
                type="plasmid.action.ConfirmResultsAction"
               name="enterResultsForm"
               validate="false"
              input="/ConfirmResultFile.jsp">
      <forward  name="success"  path="/GetResultTypes.do"/>
    </action>
    
    <action    path="/DownloadClones"
               type="plasmid.action.DownloadClonesAction"
               name="downloadClonesForm"
              input="/ViewOrderClones.jsp">
      <forward  name="success"  path="/CreateBiobankWorklistSuccess.jsp"/>>
    </action>
    
    <action     path="/EnterAddress"
                type="plasmid.action.EnterAddressAction"
               name="checkoutForm"
              input="/ChooseShipping.jsp">
      <forward  name="success"  path="/Checkout.jsp"/>
    </action>
    
    <action     path="/EnterShipping"
                type="plasmid.action.EnterShippingAction"
               name="processShippingForm"
              input="/EnterShipping.jsp">
      <forward  name="success"  path="/ViewOrderDetail.jsp"/>
    </action>
         
    <action    path="/FindPassword"
               type="plasmid.action.FindPasswordAction"
               name="findPasswordForm"
              scope="request"
              input="/FindPassword.jsp">
      <forward  name="success"   path="/FindPasswordSuccessful.jsp"/>
    </action>
    
    <action    path="/GenerateContainers"
               type="plasmid.action.GenerateContainersAction"
               name="generateWorklistForm"
              validate="false">
      <forward  name="success"   path="/GenerateContainersSuccessful.txt"/>
      <forward  name="fail"   path="/GenerateContainersFail.txt"/>
    </action>
         
    <action    path="/GetAllVectors"
               type="plasmid.action.GetAllVectorsAction"
              input="/GetAllVectors.do">
      <forward  name="success"   path="/DisplayVectors.jsp"/>
      <forward  name="empty"   path="/DisplayVectorsEmpty.jsp"/>
    </action>
         
    <action    path="/GetCloneDetail"
               type="plasmid.action.GetCloneDetailAction"
               name="cloneDetailForm"
              scope="request"
              input="/GetCloneDetailAction">
      <forward  name="success"   path="/ViewCloneDetail.jsp"/>
    </action>
     
    <action    path="/GetCloneSearchTypes"
               type="plasmid.action.GetCloneSearchTypesAction">
      <forward  name="success"   path="/SearchClone.jsp"/>
    </action>
     
    <action    path="/GetCollection"
               type="plasmid.action.GetCollectionAction"
               name="refseqSearchForm"
              validate="false"
              input="/GetCollectionList.do">
      <forward  name="success"   path="/ViewCollection.jsp"/>
    </action>
         
    <action    path="/GetCollectionList"
               type="plasmid.action.GetCollectionListAction"
              scope="request"
              input="/GetCollectionList.do">
      <forward  name="success"   path="/CollectionList.jsp"/>
      <forward  name="empty"   path="/CollectionListEmpty.jsp"/>
    </action>
     
    <action    path="/GetDataForRefseqSearch"
               type="plasmid.action.GetDataForRefseqSearchAction"
               name="refseqSearchForm"
              input="/GetDataForRefseqSearch.do">
      <forward  name="success"   path="/RefseqSearch.jsp"/>
    </action>
     
    <action    path="/GetResultTypes"
               type="plasmid.action.GetResultTypesAction"
              scope="request"
              input="/GetResultTypes.do">
      <forward  name="success"   path="/SelectResultType.jsp"/>
    </action>
     
    <action    path="/GetVectorDetail"
               type="plasmid.action.GetVectorDetailAction"
               name="vectorDetailForm"
              scope="request"
              input="/GetVectorDetailAction">
      <forward  name="success"   path="/ViewVectorDetail.jsp"/>
    </action>
     
    <action    path="/GetVectorPropertyTypes"
               name="vectorSearchForm"
               type="plasmid.action.GetVectorPropertyTypesAction"
               input="/GetVectorPropertyTypes.do">
      <forward  name="success"   path="/VectorSearch.jsp"/>
    </action>
     
    <action    path="/LabelPlates"
               type="plasmid.action.LabelPlatesAction"
               name="labelPlatesForm"
              scope="request"
              input="/LabelPlates.jsp">
      <forward  name="success"   path="/PrintLabels.jsp"/>
    </action>
    
    <!-- Process a user logon -->
    <action    path="/Logon"
               type="plasmid.action.LogonAction"
               name="logonForm"
              scope="request"
              input="/Login.jsp">
      <forward  name="success"   path="/Account.jsp"/>
      <forward  name="confirm"   path="/MergeCart.jsp"/>
      <forward  name="failure"  path="/Login.jsp"/>
    </action>

    <!-- process a user logout -->
    <action     path="/Logout"
                type="plasmid.action.LogoutAction">
      <forward  name="success"  path="/Home.jsp"/>
      <forward  name="confirm"  path="/ConfirmLogout.jsp"/>
    </action>
    
     <action    path="/MergeCart"
               type="plasmid.action.MergeCartAction"
               name="mergeCartForm"
              scope="request"
              input="/Login.jsp">
      <forward  name="success"   path="/ViewCart.do"/>
    </action>
            
    <action     path="/PlaceOrder"
                type="plasmid.action.PlaceOrderAction"
               name="checkoutForm"
              input="/CheckoutConfirm.jsp">
      <forward  name="success"  path="/ViewOrderDetail.jsp"/>
    </action>
    
    <action     path="/PrepareAdvancedSearch"
                type="plasmid.action.PrepareAdvancedSearchAction"
               name="advancedSearchForm">
      <forward  name="success"  path="/AdvancedSearch.jsp"/>
    </action>
    
    <action    path="/PrepareRegistration"
               type="plasmid.action.PrepareRegistrationAction"
              input="/PrepareRegistration.do">
      <forward  name="success"   path="/HomeRegistration.jsp"/>
    </action>
    
    <action    path="/PrintLabels"
               type="plasmid.action.PrintLabelsAction"
               name="printLabelsForm">
      <forward  name="success"   path="/PrintLabels.jsp"/>
    </action>
      
    <action     path="/ProcessError"
                type="edu.harvard.med.hip.flex.action.ProcessErrorAction">
       <forward name="displayError" path="/PlasmidError.jsp"/>
    </action>
    
    <action    path="/ProcessShipping"
               type="plasmid.action.ProcessShippingAction"
               name="processShippingForm">
      <forward  name="success"   path="/EnterShipping.jsp"/>
    </action>

    <action    path="/RefseqSearch"
               type="plasmid.action.RefseqSearchAction"
               name="refseqSearchForm"
              input="/RefseqSearch.jsp">
      <forward  name="success"   path="/RefseqSearchContinue.jsp"/>
    </action>
         
    <action    path="/RefseqSearchContinue"
               type="plasmid.action.RefseqSearchContinueAction"
               name="refseqSearchForm"
              input="/RefseqSearchContinue.jsp">
      <forward  name="success"   path="/DisplayResults.jsp"/>
    </action>
      
    <action    path="/Registration"
               type="plasmid.action.RegistrationAction"
               name="registrationForm"
               validate="true"
              input="/PrepareRegistration.do">
      <forward  name="success_confirm"   path="/ConfirmRegistration.jsp"/>
      <forward  name="success"   path="/Login.jsp"/>
    </action>
      
    <action    path="/SearchClone"
               type="plasmid.action.SearchCloneAction"
               name="cloneSearchForm"
               scope="request"
              input="/SearchClone.jsp">
      <forward  name="success"   path="/DisplayResults.jsp"/>
    </action>
      
    <action    path="/SearchOrderInput"
               type="plasmid.action.SearchOrderInputAction"
               name="searchOrderForm"
               scope="request"
              input="/SearchOrderInput.jsp">
      <forward  name="success"   path="/OrderList.jsp"/>
      <forward  name="success_empty"   path="/OrderListEmpty.jsp"/>
    </action>
      
    <action    path="/SelectProcess"
               type="plasmid.action.SelectProcessAction"
               name="generateWorklistForm"
              input="/SelectProcess.jsp"
              validate="false">
      <forward  name="success"   path="/WorklistInput.jsp"/>
      <forward  name="success_plating"   path="/PlatingInput.jsp"/>
    </action>
      
    <action    path="/SelectResultType"
               type="plasmid.action.SelectResultTypeAction"
               name="enterResultsForm"
              input="/GetResultTypes.do"
              validate="false">
      <forward  name="success_upload_result_file"   path="/UploadResultFile.jsp"/>
      <forward  name="fail"   path="/ResultTypeNotFound.jsp"/>
    </action>
         
    <action    path="/SetDisplay"
               type="plasmid.action.SetDisplayAction"
               name="refseqSearchForm"
              input="/RefseqSearchContinue.jsp">
      <forward  name="success"   path="/DisplayResults.jsp"/>
      <forward  name="success_collection"   path="/ViewCollection.jsp"/>
      <forward  name="success_vector_search"   path="/DisplayVectorSearchResults.jsp"/>
    </action>
         
    <action    path="/SetVectorDisplay"
               type="plasmid.action.SetVectorDisplayAction"
               name="refseqSearchForm"
              scope="request"
              input="/DisplayVectors.jsp">
      <forward  name="success"   path="/DisplayVectors.jsp"/>
    </action>
         
    <action    path="/UploadCultureResults"
               type="plasmid.action.UploadCultureResultsAction"
               name="uploadCultureResultsForm"
              scope="request"
              input="/UploadCultureResults.jsp">
      <forward  name="success"   path="/ConfirmCultureResults.jsp"/>
    </action>

    <action    path="/UpdateCart"
               type="plasmid.action.UpdateCartAction"
               name="viewCartForm"
              input="/DisplayCart.jsp">
      <forward  name="success"   path="/DisplayCart.jsp"/>
      <forward  name="success_empty"   path="/DisplayEmptyCart.jsp"/>
      <forward  name="success_save"   path="/Account.jsp"/>
      <forward  name="success_checkout"   path="/Checkout.do"/>
    </action>
      
    <action    path="/UploadResultFile"
               type="plasmid.action.UploadResultFileAction"
               name="enterResultsForm"
              input="/UploadResultFile.jsp"
              validate="true">
      <forward  name="success"   path="/ConfirmResultFile.jsp"/>
      <forward  name="fail"   path="/ResultTypeNotFound.jsp"/>
    </action>
      
    <action    path="/VectorSearch"
               type="plasmid.action.VectorSearchAction"
               name="vectorSearchForm"
              input="/VectorSearch.jsp">
      <forward  name="success"   path="/VectorSearchContinue.jsp"/>
    </action>
      
    <action    path="/VectorSearchContinue"
               type="plasmid.action.VectorSearchContinueAction"
               name="vectorSearchForm"
              input="/VectorSearchContinue.jsp">
      <forward  name="success"   path="/DisplayVectorSearchResults.jsp"/>
    </action>
    
    <action    path="/ViewCart"
               type="plasmid.action.ViewCartAction"
               name="viewCartForm"
              input="/ViewCart.do">
      <forward  name="success"   path="/DisplayCart.jsp"/>
      <forward  name="success_empty"   path="/DisplayEmptyCart.jsp"/>
    </action>
    
    <action    path="/ViewContainers"
               type="plasmid.action.ViewContainersAction"
               name="viewContainersForm"
               scope="request"
               validate="true"
              input="/ViewContainers.jsp">
      <forward  name="success"   path="/DisplayContainers.jsp"/>
    </action>
        
    <action    path="/ViewOrderClones"
               type="plasmid.action.ViewOrderClonesAction"
               name="viewOrderClonesForm"
               scope="request"
              input="/ViewOrderClones.do">
      <forward  name="success"   path="/ViewOrderClones.jsp"/>
    </action>
        
    <action    path="/ViewOrderCollections"
               type="plasmid.action.ViewOrderCollectionsAction"
               name="viewOrderClonesForm"
               scope="request"
              input="/ViewOrderClones.do">
      <forward  name="success"   path="/ViewOrderCollections.jsp"/>
    </action>
    
    <action    path="/ViewOrderDetail"
               type="plasmid.action.ViewOrderDetailAction"
               name="viewOrderDetailForm"
              input="/ViewOrderDetail.do">
      <forward  name="success"   path="/ViewOrderDetail.jsp"/>
    </action>
    
    <action    path="/ViewOrderHistory"
               type="plasmid.action.ViewOrderHistoryAction"
               name="viewOrderHistoryForm"
              input="/ViewOrderHistory.do">
      <forward  name="success"   path="/OrderList.jsp"/>
      <forward  name="success_outside"   path="/OrderListOutside.jsp"/>
      <forward  name="success_empty"   path="/OrderListEmpty.jsp"/>
    </action>
    
    <action    path="/WorklistInput"
               type="plasmid.action.WorklistInputAction"
               name="generateWorklistForm"
              input="/WorklistInput.jsp"
              validate="true">
      <forward  name="success"   path="/WorklistOutput.jsp"/>
    </action>
    
    <!-- The standard administrative actions available with Struts -->
    <!-- These would be either omitted or protected by security -->
    <!-- in a real application deployment -->
    <action    path="/admin/addFormBean"
               type="org.apache.struts.actions.AddFormBeanAction"/>
    <action    path="/admin/addForward"
               type="org.apache.struts.actions.AddForwardAction"/>
    <action    path="/admin/addMapping"
               type="org.apache.struts.actions.AddMappingAction"/>
    <action    path="/admin/reload"
               type="org.apache.struts.actions.ReloadAction"/>
    <action    path="/admin/removeFormBean"
               type="org.apache.struts.actions.RemoveFormBeanAction"/>
    <action    path="/admin/removeForward"
               type="org.apache.struts.actions.RemoveForwardAction"/>
    <action    path="/admin/removeMapping"
               type="org.apache.struts.actions.RemoveMappingAction"/>


  </action-mappings>

</struts-config>
